{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

    "mathbbR": {
        "prefix": "mbr",
        "body": [
            "\\mathbb{R}"
        ],
        "description": "Insert \\mathbb{R} (real numbers set)"
    },
    "mathbbN": {
        "prefix": "mbn", 
        "body": [
            "\\mathbb{N}"
        ],
        "description": "Insert \\mathbb{N} (natural numbers set)"
    },
    "mathbbZ": {
        "prefix": "mbz",
        "body": [
            "\\mathbb{Z}"
        ],
        "description": "Insert \\mathbb{Z} (integers set)"
    },
    "mathbbQ": {
        "prefix": "mbq",
        "body": [
            "\\mathbb{Q}"
        ],
        "description": "Insert \\mathbb{Q} (rational numbers set)"
    },
    "mathbbC": {
        "prefix": "mbc",
        "body": [
            "\\mathbb{C}"
        ],
        "description": "Insert \\mathbb{C} (complex numbers set)"
    },

    "map": {
        "prefix": "t",
        "body": [
            "\\to"
        ],
        "description": "to arrow"
    },

    "ell": {
        "prefix": "l",
        "body": [
            "\\ell"
        ],
        "description": "lowercase script l"
    },

    "forall": {
        "prefix": "fa",
        "body": [
            "\\forall"
        ],
        "description": "forall symbol"
    },

    "alpha": {
        "prefix": "a",
        "body": [
            "\\alpha"
        ],
        "description": "alpha symbol"
    },

    "cdots": {
        "prefix": "cd",
        "body": [
            "\\cdots"
        ],
        "description": "cdots"
    },

    "beta": {
        "prefix": "b",
        "body": [
            "\\beta"
        ],
        "description": "beta symbol"
    },

    "sup": {
        "prefix": "sup",
        "body": [
            "\\sup\\limits_{}"
        ],
        "description": "sup symbol"
    },

    "vectorx": {
        "prefix": "vx",
        "body": [
            "\\vec{x}"
        ],
        "description": "vector symbol"
    },

    "vert": {
        "prefix": "dd",
        "body": [
            "\\vert"
        ],
        "description": "vert symbol"
    },

    "partial": {
        "prefix": "dd",
        "body": [
            "\\partial"
        ],
        "description": "partial symbol"
    },

    "exp": {
        "prefix": "ss",
        "body": [
            "^{}"
        ],
        "description": "exp symbol"
    },

    "frac": {
        "prefix": "ff",
        "body": [
            "\\frac{}{}"
        ],
        "description": "frac symbol"
    },
}